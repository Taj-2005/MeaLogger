{
  "expo": {
    "name": "meal-logger-app",
    "slug": "meal-logger-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "/assets/logo.jpeg",
    "scheme": "Meal Logger",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.taj786.mealloggerapp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "/assets/logo.jpeg",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.taj786.mealloggerapp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "/assets/logo.jpeg"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "/assets/logo.jpeg",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "6f0b745c-3144-4c8f-b602-4a2d4bf03173"
      }
    },
    "owner": "taj-786"
  }
}
